

public class Solution {
	public static int findNode(LinkedListNode<Integer> head, int n) {
		// Write your code here.
        LinkedListNode<Integer> temp=head;
        int count=0;
        while(temp!=null)
        {
           
            if(temp.data==n)
            {
                return count;
                
            }
            count++;
            temp=temp.next;
        }
        return -1;
	}
}
